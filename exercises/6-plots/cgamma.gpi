set term png size 1280,960 \
font arial 28 background rgb "white"
set out "cgamma.gnuplot.png"
set view ,310
set title "|Î“(z)|"
set hidden3d
set pm3d
set grid
set xlabel "Re(z)"
set ylabel "Im(z)"
#set zlabel "|Gamma(z)|" rotate parallel
set contour both
set cntrlabel onecolor
set linetype 1 linecolor rgb "black"
zmax=6
set cntrparam levels incremental 0.0,0.25,zmax
set palette rgb 33,13,10
cut(z)=z>zmax?zmax:z
splot [][][:zmax]\
 "cgamma.dat" using 1:2:(cut($3)) with lines notitle
